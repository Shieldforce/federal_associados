<BODY>
    <DADOS>
        <INCLUI_BOLETO>
            <CODIGO_BENEFICIARIO>{CODIGO_BENEFICIARIO}</CODIGO_BENEFICIARIO>
            <TITULO>
                <NOSSO_NUMERO>{TITULO|NOSSO_NUMERO}</NOSSO_NUMERO>
                <NUMERO_DOCUMENTO>{TITULO|NUMERO_DOCUMENTO}</NUMERO_DOCUMENTO>
                <DATA_VENCIMENTO>{TITULO|DATA_VENCIMENTO}</DATA_VENCIMENTO>
                <VALOR>{TITULO|VALOR}</VALOR>
                <TIPO_ESPECIE>{TITULO|TIPO_ESPECIE}</TIPO_ESPECIE>
                <FLAG_ACEITE>{TITULO|FLAG_ACEITE}</FLAG_ACEITE>
                <DATA_EMISSAO>{TITULO|DATA_EMISSAO}</DATA_EMISSAO>
                <JUROS_MORA>
                    <TIPO>{TITULO|JUROS_MORA|TIPO}</TIPO>
                    <VALOR>{TITULO|JUROS_MORA|VALOR}</VALOR>
                </JUROS_MORA>
                <VALOR_ABATIMENTO>{TITULO|VALOR_ABATIMENTO}</VALOR_ABATIMENTO>
                <POS_VENCIMENTO>
                    <ACAO>{TITULO|POS_VENCIMENTO|ACAO}</ACAO>
                    <NUMERO_DIAS>{TITULO|POS_VENCIMENTO|NUMERO_DIAS}</NUMERO_DIAS>
                </POS_VENCIMENTO>
                <CODIGO_MOEDA>{TITULO|CODIGO_MOEDA}</CODIGO_MOEDA>
                <PAGADOR>
                    <CPF>{TITULO|PAGADOR|CPF}</CPF>
                    <NOME>{TITULO|PAGADOR|NOME}</NOME>
                    <ENDERECO>
                        <LOGRADOURO>{TITULO|PAGADOR|ENDERECO|LOGRADOURO}</LOGRADOURO>
                        <BAIRRO>{TITULO|PAGADOR|ENDERECO|BAIRRO}</BAIRRO>
                        <CIDADE>{TITULO|PAGADOR|ENDERECO|CIDADE}</CIDADE>
                        <UF>{TITULO|PAGADOR|ENDERECO|UF}</UF>
                        <CEP>{TITULO|PAGADOR|ENDERECO|CEP}</CEP>
                    </ENDERECO>
                </PAGADOR>
                <MULTA>
                    <DATA>{TITULO|MULTA|DATA}</DATA>
                    <VALOR>{TITULO|MULTA|VALOR}</VALOR>
                </MULTA>
                <DESCONTOS>
                    <DESCONTO>
                        <DATA>{TITULO|DESCONTOS|DESCONTO|1|DATA}</DATA>
                        <PERCENTUAL>{TITULO|DESCONTOS|DESCONTO|1|PERCENTUAL}</PERCENTUAL>
                    </DESCONTO>
                    <DESCONTO>
                        <DATA>{TITULO|DESCONTOS|DESCONTO|2|DATA}</DATA>
                        <PERCENTUAL>{TITULO|DESCONTOS|DESCONTO|2|PERCENTUAL}</PERCENTUAL>
                    </DESCONTO>
                    <DESCONTO>
                        <DATA>{TITULO|DESCONTOS|DESCONTO|3|DATA}</DATA>
                        <PERCENTUAL>{TITULO|DESCONTOS|DESCONTO|3|PERCENTUAL}</PERCENTUAL>
                    </DESCONTO>
                </DESCONTOS>
                <FICHA_COMPENSACAO>
                    <MENSAGENS>
                        <MENSAGEM>{TITULO|FICHA_COMPENSACAO|MENSAGENS|1|MENSAGEM}</MENSAGEM>
                        <MENSAGEM>{TITULO|FICHA_COMPENSACAO|MENSAGENS|2|MENSAGEM}</MENSAGEM>
                    </MENSAGENS>
                </FICHA_COMPENSACAO>
                <RECIBO_PAGADOR>
                    <MENSAGENS>
                        <MENSAGEM>{TITULO|RECIBO_PAGADOR|MENSAGENS|1|MENSAGEM}</MENSAGEM>
                        <MENSAGEM>{TITULO|RECIBO_PAGADOR|MENSAGENS|2|MENSAGEM}</MENSAGEM>
                        <MENSAGEM>{TITULO|RECIBO_PAGADOR|MENSAGENS|3|MENSAGEM}</MENSAGEM>
                        <MENSAGEM>{TITULO|RECIBO_PAGADOR|MENSAGENS|4|MENSAGEM}</MENSAGEM>
                    </MENSAGENS>
                </RECIBO_PAGADOR>
                <PAGAMENTO>
                    <QUANTIDADE_PERMITIDA>{TITULO|PAGAMENTO|QUANTIDADE_PERMITIDA}</QUANTIDADE_PERMITIDA>
                    <TIPO>{TITULO|PAGAMENTO|TIPO}</TIPO>
                    <VALOR_MINIMO>{TITULO|PAGAMENTO|VALOR_MINIMO}</VALOR_MINIMO>
                    <VALOR_MAXIMO>{TITULO|PAGAMENTO|VALOR_MAXIMO}</VALOR_MAXIMO>
                </PAGAMENTO>
            </TITULO>
        </INCLUI_BOLETO>
    </DADOS>
</BODY>

